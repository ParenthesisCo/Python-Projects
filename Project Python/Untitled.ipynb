   "source": [
    "import turtle\n",
    "import math\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.color(\"green\",\"yellow\")\n",
    "t.speed(10)\n",
    "\n",
    "t.begin_fill()\n",
    "for i in range(2000):\n",
    "    t.forward(math.sqrt(i))\n",
    "    t.left(i % 180)\n",
    "t.end_fill()\n",
    "\n",
    "turtle.done()"
   ]
